{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Secret": "YOUR-LONG-JWT-SECRET-HERE"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ReactMVC;Trusted_Connection=True;"
  },
  "Cloudflare": {
    "R2": {
      "AccessKey": "YOUR-ACCESS-KEY-HERE",
      "SecretKey": "YOUR-SECRET-KEY-HERE",
      "AccountId": "YOUR-ACCOUNT-ID-HERE",
      "BucketName": "YOUR-BUCKET-NAME-HERE"
    }
  },
  "Dify": {
    "ApiKey": "YOUR-API-KEY-HERE",
    "BaseUrl": "https://api.dify.ai/v1" // customize if you use self deploy
  }
}
